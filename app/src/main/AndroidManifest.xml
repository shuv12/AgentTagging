<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.com.example.android.agenttagging">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".Home"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".Agent"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".Notify"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".ViewProfile"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".NewListingPageOne"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".NewListingPageTwo"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".NewListingPageThree"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".PropertyDetails"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".UpcomingEvent"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".GroupTeam"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".FrontPage"
            android:theme="@style/AppTheme.noTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UploadPhoto"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".DisplayUploadingPhoto"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".SettingPage"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".MessagePage"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".FullscreenView"
            android:theme="@style/AppTheme.noTheme" />
        <activity
            android:name=".SearchAgent"
            android:theme="@style/AppTheme.noTheme" />
        <activity android:name=".SearchProperty"
            android:theme="@style/AppTheme.noTheme"/>
    </application>

</manifest>